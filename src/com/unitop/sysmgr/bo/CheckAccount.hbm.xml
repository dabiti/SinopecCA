<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unitop.sysmgr.bo.CheckAccount" table="CheckAccount">
		<id name="site_name" type="string" column="SITE_NAME" length="30">
			<generator class="assigned">
			</generator>
		</id>
		<property name="terminal_id" type="string" column="TERMINAL_ID" length="30"/>
		<property name="sino_amountsum" type="big_decimal" column="SINO_AMOUNTSUM" length="5"/>
		<property name="sino_poundagesum" type="big_decimal" column="SINO_POUNDAGESUM" length="30"/>
		<property name="ccb_amountsum" type="big_decimal" column="CCB_AMOUNTSUM" length="30"/>
		<property name="ccb_poundagesum" type="big_decimal" column="CCB_POUNDAGESUM" length="30"/>
		<property name="ccb_settlesum" type="big_decimal" column="CCB_SETTLESUM" length="30"/>
		<property name="seal_date" type="string" column="SEAL_DATE" length="30"/>
		<property name="seal_type" type="string" column="SEAL_TYPE" length="30"/>
		<property name="card_type" type="string" column="CARD_TYPE" length="30"/>
		<property name="seal_time" type="string" column="SEAL_TIME" length="30"/>
	</class>
</hibernate-mapping>