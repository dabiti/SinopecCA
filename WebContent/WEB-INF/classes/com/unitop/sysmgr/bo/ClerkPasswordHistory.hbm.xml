<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.unitop.sysmgr.bo.ClerkPasswordHistory" table="N_CLERKPWDHISTORYTABLE">
	   <id name="id" type="string" column="ID">
	   	<generator class="uuid.hex">
	   	</generator>
	   </id>
		<property name="clerkCode" type="string" column="CLERKNUM" length="15"/>
		<property name="password" type="string" column="PASSWORD" length="50"/>
		<property name="updateDate" type="string" column="UPDATEDATE" length="30"/>
	</class>
</hibernate-mapping>
